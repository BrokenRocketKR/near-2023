{
  "name": "near-2023",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "turbo": "latest"
  },
  "packageManager": "npm@9.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{ts,tsx,md}": "prettier --write"
  }
}
